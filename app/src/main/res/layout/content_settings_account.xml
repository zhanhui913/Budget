<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.zhan.budget.Activity.SettingsAccount"
    tools:showIn="@layout/activity_settings_account">

    <in.srain.cube.views.ptr.PtrFrameLayout
        android:id="@+id/rotate_header_list_view_frame"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <FrameLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ListView
                android:id="@+id/accountListView"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:divider="?attr/themeColorHighlight"
                android:dividerHeight="1dp"
                android:visibility="visible"
                android:drawSelectorOnTop="true"/>

            <include layout="@layout/empty_transaction_indicator"
                android:id="@+id/emptyAccountLayout"
                android:layout_width="match_parent"
                android:layout_height="match_parent"/>

        </FrameLayout>
    </in.srain.cube.views.ptr.PtrFrameLayout>

</RelativeLayout>
