<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:custom="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.zhan.budget.Fragment.OverviewFragment">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:text="@string/hello_blank_fragment" />

    <com.zhan.circularview.CircularView
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:background="@color/purple"
        custom:bg_color="@color/green"
        custom:stroke_width="20"
        custom:stroke_color="@color/black"
        custom:cv_icon="@drawable/ic_date"
        custom:icon_size="LARGE"
        custom:icon_color="@color/purple"/>
</FrameLayout>
